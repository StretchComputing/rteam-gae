<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="rteam.css" />
	
	<g:HTMLPanel styleName="{style.panel}">
		<div style="height:15px;"></div>
		<div class="{style.welcome}" >
			<span>Your poll response was successfully processed</span>
		</div>
		
		<div class="{style.userInfo}">Poll Results For <span ui:field="numOfMembers" /> Member(s)</div>
		<g:Grid ui:field="membersGrid" />
	
		<div style="margin-top:25px;">
			<div>Debug Message</div>
			<span ui:field="debugMessage" />
		</div>
	</g:HTMLPanel> 
</ui:UiBinder> 