<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="rteam.css" />
	
	<g:HTMLPanel styleName="{style.panel}">
		<div style="height:15px;"></div>
		<div class="{style.welcome}" >
			<span>Message has been confirmed as requested</span>
		</div>
		<div class="{style.userInfo}">rTeam User Information</div>
		<table id="box-table-a" summary="User Info">
		    <thead>
		    	<tr>
		        	<th scope="col">Property</th>
		            <th scope="col">Value</th>
		        </tr>
		    </thead>
		    <tbody>
		        <tr>
		        	<td>Message Recipient Name</td>
		            <td><span ui:field="fullName" /></td>
		        </tr>
		    </tbody>
		</table>
	</g:HTMLPanel> 
</ui:UiBinder> 